class Test1() {
    func Test1() {
        this.hello = "yes";
    }
    func aaa() {
        print("Hey.");
        print(this.hello);
    }
    func override_me() {
        print("You didn't");
    }
    aaaaa = 17;
}

class Test2(Test1) {
    func Test2() {
        this.hello = "Whee";
    }
    func aaa2() {
        print("Hello!");
        print(this.aaaaa);
    }
    func override_me(){
        print("I did.");
    }
}

t2 = Test2();
t2.aaa();
t2.aaa2();
t2.override_me();